{
  "name": "node-blog",
  "version": "1.0.0",
  "description": " node搭建的个人博客网站",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rimraf dist && node ./server/dev-server.js --mode development",
    "server": "cross-env NODE_ENV=production node ./server/pro-server.js --mode production",
    "test": "mocha ./test",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules --mode production",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woai3c/node-blog.git"
  },
  "keywords": [
    "node",
    "blog",
    "vue"
  ],
  "author": "woai3c",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/woai3c/node-blog/issues"
  },
  "homepage": "https://github.com/woai3c/node-blog#readme",
  "dependencies": {
    "axios": "^0.28.0",
    "babel-runtime": "^6.26.0",
    "bunyan": "^1.8.12",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lib-qqwry": "^1.3.2",
    "mongodb": "^3.2.6",
    "serve-favicon": "^2.5.0",
    "view-design": "^4.4.0",
    "vue": "^2.6.12",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "babel-loader": "^8.3.0",
    "chokidar": "^3.4.3",
    "clean-webpack-plugin": "^2.0.2",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "memory-fs": "^0.5.0",
    "mocha": "^10.1.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.1",
    "vue-server-renderer": "^2.6.12",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^2.5.2",
    "webpackbar": "^4.0.0"
  }
}
